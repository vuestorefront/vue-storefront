    "karma-webpack": "^2.0.13",
    "lerna": "^2.9.0",
    "mocha": "^3.0.2",
    "node-sass": "^4.7.2",
    "phantomjs-prebuilt": "^2.1.10",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
