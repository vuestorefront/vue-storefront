    "express": "^4.14.0",
    "fs-exists-sync": "^0.1.0",
    "he": "^1.1.1",
    "inquirer": "^3.3.0",
    "is-windows": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonfile": "^4.0.0",
    "localforage": "^1.5.0",
    "lru-cache": "^4.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.21.0",
    "npm": "^5.7.1",
    "object-hash": "^1.2.0",
    "print-message": "^2.1.0",
    "rimraf": "^2.6.0",
    "scrollreveal": "^3.3.6",
    "shelljs": "^0.8.1",
    "url-loader": "^1.0.1",
